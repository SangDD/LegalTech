<script>
    $('#6_txtNuocNopDon').multipleSelect({
        filter: true,
        noMatchesFound: "Không tìm thấy dữ liệu",
        single: true,
        isdatastring: false,
        placeholder: 'Tìm kiếm quốc gia'
    });

    $("#6_txtNuocNopDon").multipleSelect("disable");


    $("#0_ckbDacTinhHangHoa").change(function () {
        if (this.checked) { funcEnableDisable('classDacTinhHangHoa', true); } else { funcEnableDisable('classDacTinhHangHoa', false); }
    });

    function funcEnableDisable(classaction, enable) {
        if (enable == true) { $("." + classaction).removeAttr('disabled'); } else { $("." + classaction).attr('disabled', 'disabled'); }
    }

    $("#0_ckbCoSuDungDonUuTien").change(function () {
        if (this.checked) {
            funcEnableDisable('classDacTinhUT', true);

            $("#6_txtNuocNopDon").multipleSelect("enable")
        } else {
            funcEnableDisable('classDacTinhUT', false);
            $("#6_txtNuocNopDon").multipleSelect("disable")
        }
    });

    document.getElementById("pfile_6_rdoMauNhanHieu").onchange = function () {
        filePreview(this, 'imgMauNhanHieu');
    };

    function funcUpLoadMauNhanHieu(pFile) {
        try {
            var file = document.getElementsByName(pFile);
            if (file != undefined) {
                formdata.append("pInfo.pfiles", file[0].files[0]);
            }
        } catch (e) {
        }
    }

    $(document).ready(function () {
        funcEnableDisable('classDacTinhUT', false);
        $.datetimepicker.setLocale('vi');
        $('.datetimepicker').datetimepicker({
            timepicker: false,
            format: 'd/m/Y',
            formatDate: 'd/m/Y',
            mask: '39/19/9999',
            validateOnBlur: false,
            scrollInput: false
        });
    });

    function ShowHidenUpLoad(pID, pClass) {
        try {
            if ($("#" + pID).is(':checked')) {
                $('.' + pClass).css('display', 'block');
            } else {
                $('.' + pClass).css('display', 'none');
            }
        } catch (e) {
        }
    }

    $("#btnGuiDon").click(function () {
        try {
            //Thông tin chủ đơn
            var formData = new FormData();
            if (funcThongTinChuDonNguoiDaiDien(formData, "1") != true) {
                return false;
            }
            var v_AppCode = $("#txtAppCode").val();
            if (v_AppCode === "") {
                $('#txtAppCode').focus().val('');
                showError('Chưa chọn loại đơn đăng ký!');
                return false;
            }
            formData.append("pInfo.Appcode", v_AppCode);
            if (funcThongTinChuDonNguoiDaiDien(formData, "2") != true) {
                return false;
            }
            var v_RepMasterType = $("input[name='3_rdoDaiDienChuDon']:checked").val();
            if (v_RepMasterType === "" || v_RepMasterType == undefined) {
                showError('Phải chọn ít nhất 1 giá trị đại điện cho chủ đơn!');
                return false;
            }
            formData.append("pInfo.Rep_Master_Type", v_RepMasterType);
            //trang thai
            formData.append("pInfo.Status", 2);
            //Lấy thông tin detail
            var reponse = funcGetAppDetail(formData);
            if (reponse == false) {
                return false;
            }
            //Lấy thông tin file có trong đơn
            reponse = funGetThongTinFile(formData);
            if (reponse == false) {
                return false;
            }
            reponse = funcGetDataClassInfo(formData);
            if (reponse == false) {
                return false;
            }
            //lay tai lieu khac
            reponse = funcGetTenTaiLieuKhac(formData);
            if (reponse == false) {
                return false;
            }
            $.ajax({
                url: '/trade-mark/dang_ky_nhan_hieu',
                type: 'POST',
                data: formData,
                processData: false, contentType: false, traditional: true, dataType: "json",
                enctype: 'multipart/form-data', async: false, headers: { "cache-control": "no-cache" },
                success: function (data) {
                    if (data != null && data.status >= 0) {
                        jAlert(AddSuccess, ThongBao, function () {
                            window.location.reload();
                        });
                    } else {
                        jError(AddFail, ThongBao, function () {
                        });
                    }
                },
                error: function (e) {
                    console.log(e);
                }
            });

        } catch (e) {
        }

    });

    function funcGetAppDetail(formdata) {
        try {
            var v_soDon_0 = $("#0_txtDonSo").val();
            if (v_soDon_0 === "" || v_soDon_0 == undefined) {
                showError('Số đơn không được để trống');
                $("#0_txtDonSo").focus();
                return false;
            }
            formdata.append("pDetail.Appno", v_soDon_0);
            var v_NgayNopDonn_0 = $("#0_txtNgayNopDon").val();
            if (v_NgayNopDonn_0 === "" || v_NgayNopDonn_0 == undefined) {
                showError('Phải chọn ít nhất 1 giá trị cho nhãn hiệu!');
                $("#0_txtNgayNopDon").focus();
                return false;
            }
            formdata.append("pDetail.Duadate", v_NgayNopDonn_0);
            var v_NhanHieu = $("input[name='0_NhanHieu']:checked").val();
            if (v_NhanHieu === "" || v_NhanHieu == undefined) {
                showError('Phải chọn ít nhất 1 giá trị cho nhãn hiệu!');
                return false;
            }

            var fileLogo = document.getElementsByName("pfile_6_rdoMauNhanHieu");
            if (fileLogo != undefined || fileLogo != null) {
                formdata.append("pDetail.pfileLogo", fileLogo[0].files[0]);
            } else {
                showError('Mẫu nhãn hiệu không được để trống');
                $("#pfile_6_rdoMauNhanHieu").focus();
            }

            var v_DacTinh = 0;
            if ($("#0_ckbDacTinhHangHoa").prop('checked') == true) {
                v_DacTinh = 1;
            }
            formdata.append("pDetail.Dactichhanghoa", v_DacTinh);


            var v_CoSDQuyenUT = 0;
            if ($("#0_ckbCoSuDungDonUuTien").prop('checked') == true) {
                v_CoSDQuyenUT = 1;
            }
            formdata.append("pDetail.Used_Special", v_CoSDQuyenUT);

            var v_QuyenUuTien = 0;
            if (v_CoSDQuyenUT == 1) {
                v_QuyenUuTien = $("input[name='4_YCHuongQuyen']:checked").val();
                if (v_QuyenUuTien == undefined || v_QuyenUuTien == null || v_QuyenUuTien == 0) {
                    showError('Yêu cầu hưởng quyền ưu tiên phải chọn ít nhất 1 giá trị');
                    return false;
                }

                var v_Sodon_Ut = $("#6_txtSoDon").val();
                if (v_Sodon_Ut === "" || v_Sodon_Ut == undefined) {
                    showError('Số đơn ưu tiên không được để trống');
                    $("#6_txtSoDon").focus();
                    return false;
                }
                formdata.append("pDetail.Sodon_Ut", v_Sodon_Ut);

                var v_Ngaynopdon_Ut = $("#6_txtNgayNopDon").val();
                if (v_Ngaynopdon_Ut === "" || v_Ngaynopdon_Ut == undefined) {
                    showError('Ngày nộp đơn ưu tiên không được để trống');
                    $("#6_txtNgayNopDon").focus();
                    return false;
                }
                formdata.append("pDetail.Ngaynopdon_Ut", v_Ngaynopdon_Ut);


                var v_Nuocnopdon_Ut = $("#6_txtNuocNopDon").val();
                if (v_Nuocnopdon_Ut === "" || v_Nuocnopdon_Ut == undefined) {
                    showError('Nước nộp đơn ưu tiên không được để trống');
                    $("#6_txtNuocNopDon").focus();
                    return false;
                }
                formdata.append("pDetail.Nuocnopdon_Ut", v_Nuocnopdon_Ut);
            }

            formdata.append("pDetail.Huongquyenuutien", v_QuyenUuTien);

            var v_MauSac_0 = $("#0_MauSac").val();
            formdata.append("pDetail.Color", v_MauSac_0);

            var v_MoTa_0 = $("#0_MoTa").val();
            formdata.append("pDetail.Description", v_MoTa_0);

            //Lay thong tin Chu don khac 1
            var reponse = funcThongTinChuDonNguoiDaiDien(formdata, "9");
            if (reponse == false) {
                return false;
            }
