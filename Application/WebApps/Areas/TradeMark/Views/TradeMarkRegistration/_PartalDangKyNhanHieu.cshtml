    function SumFee() {
        try {
            var LePhiYCSD = $("#5_txtLePhiYCSD").val().replace(/,/g, "");
            if (LePhiYCSD === "" || LePhiYCSD == undefined) LePhiYCSD = 0;
            var LePhiCBSD = $("#5_txtLePhiCBSD").val().replace(/,/g, "");
            if (LePhiCBSD === "" || LePhiCBSD == undefined) LePhiCBSD = 0;
            var LePhiCBD = $("#5_txtLePhiCBD").val().replace(/,/g, "");
            if (LePhiCBD === "" || LePhiCBD == undefined) LePhiCBD = 0;

            var TienLePhiYCSD = $("#5_txtTienLePhiYCSD").val().replace(/,/g, "");
            if (TienLePhiYCSD === "" || TienLePhiYCSD == undefined) TienLePhiYCSD = 0;
            var TienLePhiCBSD = $("#5_txtTienLePhiCBSD").val().replace(/,/g, "");
            if (TienLePhiCBSD === "" || TienLePhiCBSD == undefined) TienLePhiCBSD = 0;
            var TienLePhiCBD = $("#5_txtTienLePhiCBD").val().replace(/,/g, "");
            if (TienLePhiCBD === "" || TienLePhiCBD == undefined) TienLePhiCBD = 0;

            var total = LePhiYCSD / 1 + LePhiCBSD / 1 + LePhiCBD / 1 + TienLePhiYCSD / 1 + TienLePhiCBSD / 1 + TienLePhiCBD / 1;
            var tongtien = formatNumberToStringN1(total);
            $("#5_lblTotalAmount").text(tongtien);
            $("#5_lblTotalAmount").css("font-weight", "bold");
        } catch (e) {
            console.log(e);
        }
    }




