<div class="d-nav-container" style="background-color:#fff">
    <div class="divCover">
        <div class="d-nav">
            <img src="/Content/icons/Body-icon-home.png">@WebApps.Resources.Resource.lblNhanHieu <span class="red">&gt;</span>
            <a href="/quan-tri-he-thong/quan-ly-nhom-quyen">@WebApps.Resources.Resource.lblDanhSachMauDon</a>
        </div>
    </div>
    <div class="divCover">
        <div class="classLine">
        </div>
    </div>
</div>

<div class="div-search ">
    <fieldset style="border: 1px solid #dddddd; padding: 1.25rem;">
        <legend>@WebApps.Resources.Resource.lblDanhSachMauDon</legend>
        @foreach (SysApplicationInfo item in listAppData)
        {
            numberApp++;
            string _url = "/trade-mark/request-for-trade-mark/" + item.Appcode.ToLower();
            if (item.URL != "")
            {
                _url = item.URL;
            }

            if (string.IsNullOrEmpty(_url))
            {
                _url = "/trade-mark/request-for-trade-mark/" + item.Appcode.ToLower();
            }
            if (numberApp % 2 == 0)
            {
                <div class="class_right">
                    <a href="@_url">(@numberApp.ToString())  @item.Appname</a>
                </div>
            }
            else
            {
                <div class="class_left">
                    <a  href="@_url">(@numberApp.ToString())  @item.Appname</a>
                </div>
            }

        }
    </fieldset>
</div>

<div id="divTableGroupUsers">
</div>

