                <div class="class_full_div_input">
                    <div class="class_radio_head"><input type="radio" name="3_DonYCSuaDoi" value="DKNH" id="3_rdoDangKyNhanHieu" placeholder="3_rdoDangKyNhanHieu" /></div>
                    <div class="class_text_end"><p class="class_text_left">Đơn đăng ký nhãn hiệu</p></div>
                </div>

                <div class="class_full_div_input">
                    <div class="class_radio_head"><input type="radio" name="3_DonYCSuaDoi" value="CDDL" id="3_rdoChiDanDaiLy" placeholder="3_rdoChiDanDaiLy" /> </div>
                    <div class="class_text_end"><p class="class_text_left">Đơn đăng ký chỉ dẫn địa lý</p></div>
                </div>
            </div>
            <div class="class_right_40 classborder40Right">
                <div class="class_block_title"><label>Số đơn:</label></div>
                <div class="class_block_title_control">
                    <input type="text" id="3_txtSoDon" placeholder="3_txtSoDon" maxlength="50" />
                </div>
            </div>
        </div>
        @* 4 *@
        <div class="div_full_100">
            <div class="class_left_40 classborder40Left">
                <div class="class_block_title numberCircle">4</div>
                <div class="class_block_title_control ">
                    <p class="class_text_center_bold ">NỘI DUNG YÊU CẦU SỬA ĐỔI</p>
