    Layout = "~/ViewsShared/_Layout.cshtml";
}
<div class="d-nav-container">
    <ul class="ul-nav">
        <li><a href="javascript:;">TradeMark</a></li>
        <li><a href="javascript:;"> Request for amendment of application)</a></li>
    </ul>
</div>

<div class="div-search ">
    <fieldset style="border: 1px solid #dddddd; padding: 1.25rem;">
        <legend>Request for amendment of application (Vietnamese)</legend>
        <p class="class_header" style="color:#666">Phụ lục B - Mẫu số: 01-SĐDD</p>

        <div class="div_full_100">
            <div class="class_left_50">
                <p class="class_text_center_bold">TỜ KHAI</p>
                <p class="class_text_center"> SỬA ĐỔI ĐƠN ĐĂNG KÝ</p>
                <p class="class_text_center">  ĐỐI TƯỢNG SỞ HỮU CÔNG NGHIỆP</p>
                <p>Kính gửi: Cục Sở hữu trí tuệ</p>
                <p style="padding-left:10%">386 Nguyễn Trãi, Hà Nội</p>
