    function GetThongTinDaiDien(vChoise) {
        try {
            var id = $("#txtAppHeaderID").val();
            var appCode = $("#txtAppCode").val();
            var status = $("#txtStatusEdit").val();
            var keyData = $("#txtKeyData").val();
            $.ajax({
                url: '/trade-mark/funcGetTTDHSH/',
                data: {
                    pKeyData: keyData, pDDSH: vChoise,  pEdit: 1, pID:id, pStatus:status
                },
                type: 'POST',
                async: false, cache: false, traditional: true,
                success: function (data) {
                    if (data != null) {
                        if (vChoise == "DDPL") {
                            $("#divDDPLChoise").html(data);
                            $("#divDDSHChoise").html("");
                            $("#divDDUQChoise").html("");

                        }
                        else if (vChoise == "DDSH") {
                            $("#divDDSHChoise").html(data);
                            $("#divDDPLChoise").html("");
                            $("#divDDUQChoise").html("");
                        }
                        else if (vChoise == "DDUQ") {
                            $("#divDDUQChoise").html(data);
                            $("#divDDPLChoise").html("");
                            $("#divDDSHChoise").html("");
                        }
                        if (vChoise == $("#txtRepMasterTypeEdit").val()) {
                            $("#txtMADDSHCN").val($("#txtRepMasterTypeEdit").val());
                        }
                        var txtTenDayDu_02 = $("#2_txtTenDayDu").val();
                        $.ajax({
                            url: '/trade-mark/GetNameSuggestKhac/',
                            type: 'POST',
                            data: {
                                pName: txtTenDayDu_02, pNumber: 5
                            },
                            sync: true, cache: false, traditional: true,
                            success: function (data) {
                                var lts = data.lst;
                                $('#2_txtTenDayDu').autocomplete({
                                    autoFocus: true,
                                    source: function (request, response) {
                                        var matcher = new RegExp("" + $.ui.autocomplete.escapeRegex(request.term.toUpperCase()), "i");
                                        response($.grep(lts, function (item) {
                                            return matcher.test(item.value);
                                        }));
                                    },
                                    select: function (event, ui) {
                                        $("#2_txtTenDayDu").val("");
                                        event.preventDefault();
                                        funcChuDonKhacReuse02(ui.item.value);
                                    },
                                    focus: function (event, ui) {
                                        var arrResult = $(".ui-menu-item");
                                        for (var i = 0; i < arrResult.length; i++) {
                                            var label = $.trim($(arrResult[i]).text().split("-")[0]);
                                            if (label == ui.item.name) {
                                                $(".clsLiActive").removeClass("clsLiActive")
                                                $(arrResult[i]).addClass("clsLiActive");
                                                break;
                                            }
                                        }
                                        event.preventDefault();
                                    },
                                    open: function (event, ui) {
                                        $("ul").scrollTop(0);
                                        event.preventDefault();
                                    },
                                });

                            }
                        });
                    }
                }
            });
        } catch (e) {
        }
    }

