
@using ObjectInfos.ModuleTrademark
@model AppDocumentInfo
@{
    var ObjDocInfo = new AppDocumentInfo();
    if (Model != null)
    {
        ObjDocInfo = (AppDocumentInfo)Model;
    }
    if (ObjDocInfo.styleCheck == null)
    {
        ObjDocInfo.styleCheck = "";
    }
}
<style>
    .classDocOtherE {
        text-align: left;
        padding: 3px;
        color: blue;
    }
</style>
@if (ObjDocInfo.styleCheck.Contains("checked") && !string.IsNullOrEmpty(ObjDocInfo.Filename))
{
    <div class="div-search-ad " style="display:inline-flex;" id="@ObjDocInfo.Id">
        <div class="div-search-content">
            <div class="div-checkbox" style="width:550px">
                <div id="" class="classDocOtherE @ObjDocInfo.Id"> <a style="color:blue;border:none" href="@ObjDocInfo.Url_Hardcopy">@ObjDocInfo.Filename</a></div>
            </div>
        </div>
        <div class="div-search-content">
            <div class="div-checkbox" style="width: 160px">
                <img onclick="RemoveFileEdit('@ObjDocInfo.Id','@ObjDocInfo.divUpLoadFile')" class="@ObjDocInfo.Id" src="~/Content/icons/delete_file.png" style="margin: 9px 5px 5px 9px;" />
                <i class="fa fa-undo" onclick="UndoFileEdit('@ObjDocInfo.Id','@ObjDocInfo.divUpLoadFile')" style="margin: 9px 5px 5px 9px; color:green;font-weight:bold" placeholder="undo" aria-hidden="true"></i>
            </div>
        </div>
    </div>
}
