<script>


    $('#6_txtNuocNopDon').multipleSelect({
        filter: true,
        noMatchesFound: "Không tìm thấy dữ liệu",
        single: true,
        isdatastring: false,
        placeholder: 'Tìm kiếm quốc gia'
    });

    $("#6_txtNuocNopDon").multipleSelect("disable");


    $("#0_ckbDacTinhHangHoa").change(function () {
        if (this.checked) { funcEnableDisable('classDacTinhHangHoa', true); } else { funcEnableDisable('classDacTinhHangHoa', false); }
    });

    function funcEnableDisable(classaction, enable) {
        if (enable == true) { $("." + classaction).removeAttr('disabled'); } else { $("." + classaction).attr('disabled', 'disabled'); }
    }

    $("#0_ckbCoSuDungDonUuTien").change(function () {
        if (this.checked) {
            funcEnableDisable('classDacTinhUT', true);
            $("#6_txtNuocNopDon").multipleSelect("enable")
        } else {
            funcEnableDisable('classDacTinhUT', false);
            $("#6_txtNuocNopDon").multipleSelect("disable")
        }
    });

    document.getElementById("pfile_6_rdoMauNhanHieu").onchange = function () {
        filePreview(this, 'imgMauNhanHieu');
    };

    function funcUpLoadMauNhanHieu(pFile) {
        try {
            var file = document.getElementsByName(pFile);
            if (file != undefined) {
                formdata.append("pInfo.pfiles", file[0].files[0]);
            }
        } catch (e) {
        }
    }

    $(document).ready(function () {
         
        if ($("#6_rdoGiayChuyenNhungUT").is(':checked')) {
            ShowHidenUpLoad("6_rdoGiayChuyenNhungUT", "6_rdoGiayChuyenNhungUT");
        }
        if ($("#6_rdoBDTVGom").is(':checked')) {
            ShowHidenUpLoad("6_rdoBDTVGom", "6_rdoBDTVGom");
        }
        if ($("#6_rdoBSDDT").is(':checked')) {
            ShowHidenUpLoad("6_rdoBSDDT", "6_rdoBSDDT");
        }

        if ($("#0_ckbCoSuDungDonUuTien").is(':checked')) {
            console.log('vao dya');
            funcEnableDisable('classDacTinhUT', true);
            $("#6_txtNuocNopDon").multipleSelect("enable")
        }
        if ($("#0_ckbDacTinhHangHoa").is(':checked')) {
            funcEnableDisable('classDacTinhHangHoa', true);
        }

        $.datetimepicker.setLocale('vi');
        $('.datetimepicker').datetimepicker({
            timepicker: false,
            format: 'd/m/Y',
            formatDate: 'd/m/Y',
            mask: '39/19/9999',
            validateOnBlur: false,
            scrollInput: false
        });
    });

    function ShowHidenUpLoad(pID, pClass) {
        try {
            if ($("#" + pID).is(':checked')) {
                $('.' + pClass).css('display', 'block');
            } else {
                $('.' + pClass).css('display', 'none');
            }
        } catch (e) {
        }
    }

    function funcPreViewApplication() {
        if ($('#divWrapperPreview').css('display') == 'block') {
            $('#divWrapperPreview').css('display', 'none');
            return;
        }
        $.ajax({
            type: "POST",
            url: "/trade-mark/Pre-View",
            headers: { "cache-control": "no-cache" },
            async: false,
            success: function (data) {
                if (data != null) {
                    $("#dpop-preview").html(data);
                    ShowPopupDialog_Ipace('divWrapperPreview', "Xem thông tin đơn", 900, 900, "");
                    $("#divWrapperPreview").css('margin-top', '-760px');
                }
                return false;
            }
        });
    }
    

</script>
