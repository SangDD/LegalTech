        try {
            var file = document.getElementsByName(pFile);
            if (file != undefined) {
                formdata.append("pInfo.pfiles", file[0].files[0]);
            }
        } catch (e) {
        }
    }

    function funcAddRemoveDisabled(pIDControl, ptxtDisable) {
        try {
            if ($("#" + pIDControl).is(':checked')) {
                $("#" + ptxtDisable).removeAttr('disabled');
            } else {
                $("#" + ptxtDisable).attr('disabled', 'disabled');
            }
        } catch (e) {

        }
    }


