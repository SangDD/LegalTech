@{
    ViewBag.Title = "Đăng ký nhãn hiệu";
    Layout = "~/ViewsShared/_Layout.cshtml";
    string language = AppsCommon.GetCurrentLang();
    var clstAppClassShortSuggest = new List<SuggestInfo>();
    foreach (var item in MemoryData.clstAppClassSuggest)
    {
        var pShortInfo = new SuggestInfo("", "");

        if (language == Language.LangVI)
        {
            if (!string.IsNullOrEmpty(item.name) && !string.IsNullOrEmpty(item.value))
            {
                pShortInfo = new SuggestInfo(item.name.Replace('/', ' ') + "[" + item.value + "]", item.value);
            }
        }
        else
        {
            if (!string.IsNullOrEmpty(item.name_en) && !string.IsNullOrEmpty(item.value))
            {
                pShortInfo = new SuggestInfo(item.name_en.Replace('/', ' ') + "[" + item.value + "]", item.value);
            }
        }
        clstAppClassShortSuggest.Add(pShortInfo);
    }
    List<AllCodeInfo> _lstLogoType = WebApps.CommonFunction.AppsCommon.AllCode_GetBy_CdTypeCdName("NOTICE_APP", "LOGOTYPE");

    string _Search_Code = "";
    if (ViewBag.SearchCode != null)
    {
        _Search_Code = (string)ViewBag.SearchCode;
    }

    string _Logourl = "";
    if (ViewBag.Logourl != null)
    {
        _Logourl = (string)ViewBag.Logourl;
    }

    int _Logochu = 0;
    if (ViewBag.Logochu != null)
    {
        _Logochu = (int)ViewBag.Logochu;
    }

    string urlMauNhanHieu = "/Content/icons/logo.jpg";
    string urlLogoChu = "";
    if (!string.IsNullOrEmpty(_Logourl) && _Logochu != 1)
    {
        urlMauNhanHieu = _Logourl.Replace("~", "");
    }
    if (!string.IsNullOrEmpty(_Logourl) && _Logochu == 1)
    {
        urlLogoChu = _Logourl;
    }


    List<Sys_Document_Info> _lst_doc = WebApps.CommonFunction.AppsCommon.Sys_Document_GetBy_Casecode("F04");
}

