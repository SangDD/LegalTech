    function funGetData  (p_Status, _UpdateToDo)
    {
        try {
            var _showerror = 0;
            if (p_Status == 2) {
                _showerror = 1;
            }
            var formData = new FormData();


            var reponse = funcThongTinCaseName(formData, "0");
            if (reponse == false) {
                return false;
            }


            var cbo_04nh_data = $("#cbo_04nh_data");

            var v_RepMasterType = $("input[name='_2_DaiDienChuDon']:checked").val();
            if ((v_RepMasterType == "" || v_RepMasterType == undefined) && _showerror == 1 ) {
                showError('@Resource.TM2B_ChonDaiDienChuDon');
                $("#_2_rdoNguoiDaiDienPL").focus();
                return false;
            }

            //Lấy thông tin detail
            var reponse = funcGetAppDetail(formData, _showerror);
            if (reponse == false) {
                return false;
            }
            var _1_txtNgayNopDon = $("#_1_txtNgayNopDon");
            formData.append("pInfo.Master_Type", "");
            formData.append("pInfo.Rep_Master_Type", v_RepMasterType);
            formData.append("pInfo.Send_Date", _1_txtNgayNopDon.val());
            //trang thai
            formData.append("pInfo.Status", p_Status);
            // thông tin chủ đơn
            formData.append("pInfo.Id", @objAppHeaderInfo.Id);
            formData.append("pInfo.Case_Code", '@objAppHeaderInfo.Case_Code');
            //Lấy thông tin file có trong đơn
            reponse = funGetThongTinFile(formData);
            if (reponse == false) {
                return false;
            }

            reponse =  funcCheckDataClassInfo(_showerror);
            if (reponse == false) {
                return false;
            }
            reponse = funcGetDataClassInfo(formData);
            if (reponse == false) {
                return false;
            }
            if (funcThongTinChuDon(formData, "1") != true) {
                return false;
            }

            if (funcThongTinDaiDienChuDon(formData, "2") != true) {
                return false;
            }
            //Lấy thông tin tài liệu có trong đơn
            reponse = funGetDocumentFile(formData);
            if (reponse == false) {
                return false;
            }
         
            //Tài liệu khác trong đơn
            reponse = funcGetTenTaiLieuKhac(formData);
            if (reponse == false) {
                return false;
            }
            reponse = funcGetCamKetChuDon(formData);
