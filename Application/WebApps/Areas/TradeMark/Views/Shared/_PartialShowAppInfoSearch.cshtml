@using ObjectInfos
@model List<AppClassInfo>
@using System.Collections;
@using WebApps.Resources;
@{
       <select id="NVSCBO001_AppClassSearch">
           @foreach (var item in Model)
           {
               <option meta-data="@item.Code" value="@item.Code">
                   @item.DisplayValue
               </option>
           }
       </select>
  
  <script>
      $('#NVSCBO001_AppClassSearch').multipleSelect({
          filter: true,
          noMatchesFound: "@Resource.lblKhongTimThayDL",
          single: true,
          isdatastring: false,
          placeholder: '@Resource.SearchAppClass'
      });
  </script>
}
