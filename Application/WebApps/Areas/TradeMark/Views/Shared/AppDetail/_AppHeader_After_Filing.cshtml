<script>
    function ShowViewApp() {
        if (CheckSessionTimeOut() == false) {
            return false;
        }
        try {
            var url = "/trade-mark/request-for-trade-mark-view/@_AppHeaderInfo.Id/@_AppHeaderInfo.Status.ToString()/@_AppHeaderInfo.Appcode";
            href="/trade-mark/request-for-trade-mark-edit/@_AppHeaderInfo.Id/@_AppHeaderInfo.Status.ToString()/@_AppHeaderInfo.Appcode"
            window.open(url);
            //location.href = url;
        } catch (e) {
            console.log(e.toString())
        }
    }


    function ChuyenNhuong(_App_No) {
        if (CheckSessionTimeOut() == false) {
            return false;
        }
        try {
            var url = "/trade-mark-3c/register/TM_PLB02CGD/" + _App_No;
            window.open(url);
            //location.href = url;
        } catch (e) {
            console.log(e.toString())
        }
    }

    function SuaDoi(_App_No) {
        if (CheckSessionTimeOut() == false) {
            return false;
        }
        try {
            var url = "/trade-mark-3b/register/TM_PLB01SDD/" + _App_No;
            window.open(url);
        } catch (e) {
            console.log(e.toString())
        }
    }

    function KhieuKien(_App_No) {
        if (CheckSessionTimeOut() == false) {
            return false;
        }
        try {
            var url = "/trade-mark-3d/register/TM_PLC05KN/" + _App_No;
            window.open(url);
        } catch (e) {
            console.log(e.toString())
        }
    }

    function ShowEditApp() {
        if (CheckSessionTimeOut() == false) {
            return false;
        }
        try {
            var url = "/trade-mark/request-for-trade-mark-edit/@_AppHeaderInfo.Id/@_AppHeaderInfo.Status.ToString()/@_AppHeaderInfo.Appcode";
            window.open(url);
            //location.href = url;
        } catch (e) {
            console.log(e.toString())
        }
    }
</script>