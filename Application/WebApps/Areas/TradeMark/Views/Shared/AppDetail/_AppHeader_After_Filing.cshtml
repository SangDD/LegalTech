@using WebApps.CommonFunction
@using ObjectInfos
@{
    var _AppHeaderInfo = new ApplicationHeaderInfo();
    _AppHeaderInfo.Case_Code = "";
    _AppHeaderInfo.Appcode = "";
    if (ViewBag.objAppHeaderInfo != null)
    {
        _AppHeaderInfo = (ApplicationHeaderInfo)ViewBag.objAppHeaderInfo;
    }

    decimal c_operator_type = Convert.ToDecimal(Common.CommonData.CommonEnums.Operator_Type.Update);
    if (ViewBag.Operator_Type != null)
    {
        c_operator_type = (decimal)ViewBag.Operator_Type;
    }

    string _Filing_Date = _AppHeaderInfo.Filing_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Filing_Date.ToString("dd/MM/yyyy");
    string _Accept_Date = _AppHeaderInfo.Accept_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Accept_Date.ToString("dd/MM/yyyy");
    string _Public_Date = _AppHeaderInfo.Public_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Public_Date.ToString("dd/MM/yyyy");

    string _Accept_Content_Date = _AppHeaderInfo.Accept_Content_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Accept_Content_Date.ToString("dd/MM/yyyy");


    string _Grant_Date = _AppHeaderInfo.Grant_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Grant_Date.ToString("dd/MM/yyyy");
    string _Grant_Public_Date = _AppHeaderInfo.Grant_Public_Date == DateTime.MinValue ? "" : _AppHeaderInfo.Grant_Public_Date.ToString("dd/MM/yyyy");

}
