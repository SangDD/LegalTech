        }
    }

    function funGetData(ptype) {
        try {
            //Thông tin chủ đơn
            var formData = new FormData();

            formData.append("pInfo.Search_Code", '@_Search_Code');
            formData.append("pInfo.Status", ptype);
            var v_AppCode = $("#txtAppCode").val();
            if (v_AppCode === "") {
                $('#txtAppCode').focus().val('');
                showError('@Html.Raw(Resource.TM3B_ChonLoaiDonDK)');
                return false;
            }

            var reponse = getNation_Represent(formData);
            if (reponse == false) {
                return false;
            }

            var reponse = funcThongTinNhapHo(formData);
            if (reponse == false) {
                return false;
            }

            var reponse = funcThongTinCaseName(formData, "0");
            if (reponse == false) {
                return false;
            }

            // box 2
            if (ptype == 1) {
                if (funcThongTin_No2_SaveTem(formData, "1") != true) {
                    return false;
                }
            } else {
                if (funcThongTin_No2(formData, "1") != true) {
                    return false;
                }
            }

            if (funcChuDonKhac(formData) != true) {
                return false;
            }

            /// box 3
            if (ptype == 1) {
                if (funcThongTin_No3_SaveTem(formData, "1") != true) {
                    return false;
                }
            } else {
                if (funcThongTin_No3(formData, "1") != true) {
                    return false;
                }
            }

            if (funcOther_BoxNo3(formData) != true) {
                return false;
            }

            // end box 3


            // Box 4 đại diện chủ đơn
            formData.append("pInfo.Appcode", v_AppCode);
            if (funcThongTinDaiDienChuDon(formData, "2") != true) {
