    function funcOther_BoxNo3(formData) {
        try {
            // bắt đầu từ 1 vì thằng 0 là bên chính
            var _index = 1;
            $(".clsInventor_No3").each(function (item) {
                var _id = $(this).attr('data-id');
                if ($("#" + _id + "_txtUse_Inventor_No3").val() == "1") {

                    formData.append("pInventor_Info[" + _index + "].Inventor_Type", 0);
                    formData.append("pInventor_Info[" + _index + "].Inventor_Name", $("#" + _id + "_txtTenDayDu_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Address", $("#" + _id + "_txtDiaChi_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Phone", $("#" + _id + "_txtDienThoai_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Fax", $("#" + _id + "_txtFax_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Email", $("#" + _id + "_txtEmail_Lao_No3_Other").val());

                    formData.append("pInventor_Info[" + _index + "].Country_Nationality", $("#" + _id + "_txtCountry_Nationality_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Country_Residence", $("#" + _id + "_txtCountry_Residence_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Country_Incorporation", $("#" + _id + "_txt_Country_Incorporation_No3_Other").val());

                    _index = _index + 1;
                }
            });
            return true;
        } catch (e) {
            return false;
        }
    }

    function funcChuDonKhac(formData) {
        try {
            var _index = 0;
            $(".clsChuDonKhac").each(function (item) {
                var _id = $(this).attr('data-id');
                if ($("#" + _id + "_txtUse_ChuDonKhac_No2").val() == "1") {

                    formData.append("pOther_MasterInfo[" + _index + "].Master_Name", $("#" + _id + "_txtTenDayDu_Lao_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Master_Address", $("#" + _id + "_txtDiaChi_Lao_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Master_Phone", $("#" + _id + "_txtDienThoai_Lao_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Master_Fax", $("#" + _id + "_txtFax_Lao_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Master_Email", $("#" + _id + "_txtEmail_Lao_No2_Other").val());

                    formData.append("pOther_MasterInfo[" + _index + "].Country_Nationality", $("#" + _id + "_txtCountry_Nationality_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Country_Residence", $("#" + _id + "_txtCountry_Residence_No2_Other").val());
                    formData.append("pOther_MasterInfo[" + _index + "].Country_Incorporation", $("#" + _id + "_txt_Country_Incorporation_No2_Other").val());

                    _index = _index + 1;
                }
            });
            return true;
        } catch (e) {
