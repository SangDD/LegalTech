                    formData.append("pInventor_Info[" + _index + "].Inventor_Address", $("#" + _id + "_txtDiaChi_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Phone", $("#" + _id + "_txtDienThoai_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Fax", $("#" + _id + "_txtFax_Lao_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Inventor_Email", $("#" + _id + "_txtEmail_Lao_No3_Other").val());

                    formData.append("pInventor_Info[" + _index + "].Country_Nationality", $("#" + _id + "_txtCountry_Nationality_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Country_Residence", $("#" + _id + "_txtCountry_Residence_No3_Other").val());
                    formData.append("pInventor_Info[" + _index + "].Country_Incorporation", $("#" + _id + "_txt_Country_Incorporation_No3_Other").val());

