            if (pType == 2) {
                if (txt_Basic_Fee == "") {
                    jError("Basic Fee can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#txt_Basic_Fee").focus()
                    });
                    return false;
                }
            }
            formData.append("pDetail.Basic_Fee", txt_Basic_Fee.replace(/,/g, ""));

            var txt_Others_Fee = $("#txt_Others_Fee").val();
            if (pType == 2) {
                if (txt_Others_Fee == "") {
                    jError("Others Fee can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#txt_Others_Fee").focus()
                    });
                    return false;
                }
            }
            formData.append("pDetail.Others_Fee", txt_Others_Fee.replace(/,/g, ""));

            // bõ IX sign
            var txt_Name_Of_Print = $("#txt_Name_Of_Print").val();
            if (pType == 2) {
                if (txt_Name_Of_Print == "") {
                    jError("Name of print can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#txt_Name_Of_Print").focus()
                    });
                    return false;
                }
            }
            formData.append("pDetail.Name_Of_Print", txt_Name_Of_Print);

            var txt_Date_Signed = $("#txt_Date_Signed").val();
            if (pType == 2) {
                if (txt_Date_Signed == "") {
                    jError("Date signed can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#txt_Date_Signed").focus()
                    });
                    return false;
                }
            }
            formData.append("pDetail.Date_Signed", txt_Date_Signed);
        } catch (e) {
            console.log(e);
            showError('@Resource.TM3B_InternetError');
            return false;
        }
    }

    function fun_Get_Don_UT(formData, pType) {
        try {
            // đơn ưu tiên
            var v_CoSDQuyenUT = 0;
            if ($("#0_ckbCoSuDungDonUuTien").prop('checked') == true) {
                v_CoSDQuyenUT = 1;
            }
            formData.append("pDetail.Used_Special", v_CoSDQuyenUT);

            if (v_CoSDQuyenUT == 1) {

                _index = 0;
                var _index_document = 12;

                $(".clsDivDonUTien").each(function (item) {
                    var _id = $(this).attr('data-id');
                    if ($("#" + _id + "_UT_Use").val() == "1") {

                        if (pType == 2) {
                            if ($("#" + _id + "_UT_txtSoDon").val() === "" || $("#" + _id + "_UT_txtSoDon").val() == undefined) {
