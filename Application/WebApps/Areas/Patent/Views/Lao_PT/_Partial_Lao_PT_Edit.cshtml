    function funcGetAppDetail(formData, pType) {
        try {

            formData.append("pDetail.Id", '@_App_Detail.Id');
            formData.append("pDetail.Case_Code", '@_App_Detail.Case_Code');
            formData.append("pDetail.App_Header_Id", '@_App_Detail.App_Header_Id');
            formData.append("pDetail.Language_Code", '@_App_Detail.Language_Code');
            formData.append("pDetail.App_No", '@_App_Detail.App_No');

            var txt_Title = $("#txt_Title").val();
            if (pType == 2) {
                if (txt_Title == "") {
                    jError("Title can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#txt_Title").focus()
                    });
                    return false;
                }
            }
            formData.append("pDetail.Title", txt_Title);

            if ($("#1_rdPattent").is(':checked')) {
                formData.append("pDetail.Patent_Type", "PT");
            } else if ($("#1_rdPetty_Pattent").is(':checked')) {
                formData.append("pDetail.Request", "PPT");
            } else if ($("#1_rdIndustrial").is(':checked')) {
                formData.append("pDetail.Request", "ID");
            }

            if ($("#1_rdPattent").is(':checked') || $("#1_rdPetty_Pattent").is(':checked')) {
                var txt_Request = $("#txt_Request_Pattent").val();
                if (pType == 2) {
                    if (txt_Request == "") {
                        jError("Request can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Request_Pattent").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Request", txt_Request);

                var txt_Claims = $("#txt_Claims").val();
                if (pType == 2) {
                    if (txt_Claims == "") {
                        jError("Claims can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Claims").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Claims", txt_Claims);

                var txt_Abstract = $("#txt_Abstract").val();
                if (pType == 2) {
                    if (txt_Abstract == "") {
                        jError("Abstract can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Abstract").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Abstract", txt_Abstract);

                var txt_Description = $("#txt_Description").val();
                if (pType == 2) {
                    if (txt_Description == "") {
                        jError("Abstract can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Description").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Description", txt_Description);

            } else if ($("#1_rdIndustrial").is(':checked')) {
                var txt_Request = $("#txt_Request_Industrial").val();
                if (pType == 2) {
                    if (txt_Request == "") {
                        jError("Request can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Request_Industrial").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Request", txt_Request);

                var txt_Number_Industrial = $("#txt_Number_Industrial").val();
                if (pType == 2) {
                    if (txt_Number_Industrial == "") {
                        jError("Number of industrial design can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Number_Industrial").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Number_Industrial", txt_Number_Industrial);

                var txt_Number_Sheet = $("#txt_Number_Sheet").val();
                if (pType == 2) {
                    if (txt_Number_Sheet == "") {
                        jError("Number of sheet of presentation can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Number_Sheet").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Number_Sheet", txt_Number_Sheet);


                if ($("#4_rdMultiple").is(':checked')) {
                    formData.append("pDetail.Multiple", "T");
                } else {
                    formData.append("pDetail.Multiple", "F");
                }

                if ($("#4_rdSetOfArticle").is(':checked')) {
                    formData.append("pDetail.SetOfArticle", "T");
                } else {
                    formData.append("pDetail.SetOfArticle", "F");
                }

                if ($("#4_rdComposition").is(':checked')) {
                    formData.append("pDetail.Composition", "T");
                } else {
                    formData.append("pDetail.Composition", "F");
                }
            }

            if ($("#1_rdPattent").is(':checked') || $("#1_rdPetty_Pattent").is(':checked')) {
                var txt_Class_Pattent = $("#txt_Class_Pattent").val();
                if (pType == 2) {
                    if (txt_Class_Pattent == "") {
                        jError("Number of sheet of presentation can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Class_Pattent").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Class_Content", txt_Class_Pattent);

            } else if ($("#1_rdIndustrial").is(':checked')) {
                var txt_Class_Industrial = $("#txt_Class_Industrial").val();
                if (pType == 2) {
                    if (txt_Class_Industrial == "") {
                        jError("Number of sheet of presentation can not be blank", "@Html.Raw(Resource.TM3B_Loi)", function () {
                            $("#txt_Class_Industrial").focus()
                        });
                        return false;
                    }
                }
                formData.append("pDetail.Class_Content", txt_Class_Industrial);
            }

            // Box V đơn ưu tiên
            reponse = fun_Get_Don_UT(formData, pType);
            if (reponse == false) {
