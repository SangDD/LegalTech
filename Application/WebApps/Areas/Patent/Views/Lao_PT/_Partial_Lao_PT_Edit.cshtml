            </div>

            <div class="div-search-ad">
                <div class="div-search-content">
                    <div class="div-radio" style="width:382px">
                        @if (_App_Detail.Multiple == "T")
                        {
                            <input type="checkbox" name="4_rd_Application_For" id="4_rdMultiple" value="1" checked>
