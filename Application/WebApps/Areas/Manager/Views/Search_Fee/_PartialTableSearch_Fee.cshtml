@using Common.CommonData
@using Common.Extensions
@using ObjectInfos;
@{
    List<Sys_Search_Fix_Info> _list_data = new List<Sys_Search_Fix_Info>();
    if (ViewBag.Obj != null)
    {
        _list_data = ViewBag.Obj;
    }
}

<div>
    <table class="table-lst-data" id="tableListGroupUser">
        <thead>
            <tr>
                <th class="class30">STT</th>
                <th class="class100">Quốc tịch</th>
                <th class="class100">Đối tượng</th>
                <th class="class100">Loại tìm kiếm</th>
                <th class="class150">Phí (VNĐ)</th>
                <th class="class150">Phí (USD)</th>
                <th >Chức năng</th>
            </tr>
        </thead>
        <tbody>
            @if (_list_data.Count > 0)
            {
                foreach (var item in _list_data)
                {
                    <tr>
                        <td class="tdcenteralign">@item.STT</td>
                        <td class="tdleftalign">@item.Country_Name</td>
                        <td class="tdleftalign">@item.Search_Object_Name</td>
                        <td class="tdcenteralign">@item.Search_Type_Name</td>
                        <td class="tdcenteralign">@item.Amount.ToString("#,##0.##")</td>
                        <td class="tdcenteralign">@item.Amount_usd.ToString("#,##0.##")</td>

                        <td class="tdcenteralign">
                            <a href="javascript:;" onclick="GetView2View(@item.Id)" class="icon icon-info" data-title="Xem chi tiết"></a>
                            <a href="javascript:;" onclick="getView2Edit(@item.Id)" class="icon icon-edit" data-title="Sửa"></a>
                            <a href="javascript:;" onclick="doDelete(@item.Id)" class="icon icon-delete" data-title="Xóa"></a>
                        </td>
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="7">Không có dữ liệu</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<div class="_ClassPage">
    <ul>
        @Html.Raw(ViewBag.Paging)
    </ul>
</div>
